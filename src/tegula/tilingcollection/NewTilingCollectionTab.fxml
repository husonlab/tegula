<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="borderPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="tegula.tilingcollection.TilingCollectionTabController">
    <top>
        <AnchorPane>
            <children>
                <VBox fx:id="topVBox" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                      AnchorPane.topAnchor="0.0">
                    <children>

                        <ToolBar fx:id="mainToolBar" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
                                 minHeight="-Infinity" prefHeight="36.0"/>
                    </children>
                </VBox>
                <Slider fx:id="sizeSlider" max="1024.0" min="32.0" snapToTicks="true" value="128.0"
                        AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0"/>
            </children>
        </AnchorPane>

    </top>
    <center>
        <AnchorPane fx:id="mainAnchorPane" BorderPane.alignment="CENTER"/>
    </center>
</BorderPane>
